<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="ancStart" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TestCAC.UIManager">
   <children>
      <VBox fx:id="vbxBox" alignment="CENTER" layoutX="250.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <children>
            <Button fx:id="btnReturn" alignment="CENTER" mnemonicParsing="false" text="Returning User" onAction="#handleReturnButtonAction">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Button>
            <Button fx:id="btnCreate" alignment="CENTER" mnemonicParsing="false" prefWidth="100.0" text="Create a User" onAction="#handleCreateButtonAction">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Button>
            <Button fx:id="btnHelp" alignment="CENTER" mnemonicParsing="false" prefWidth="100.0" text="Help">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Button>
            <Button fx:id="btnCredits" alignment="CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="100.0" text="Credits">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Button>
         </children>
      </VBox>
      <Label fx:id="lblTitle" layoutX="240.0" layoutY="43.0" text="Main Menu">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
