<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="vbxBox" alignment="CENTER" layoutX="235.0" layoutY="96.0" prefHeight="222.0" prefWidth="150.0">
         <children>
            <TextField fx:id="txtName" promptText="Username">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtPass" promptText="Password">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtRePass" promptText="Retype Password">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="btnCreate" alignment="CENTER" mnemonicParsing="false" text="Create">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </Button>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </VBox>
      <Label fx:id="lblTitle" layoutX="221.0" layoutY="46.0" text="Create New User">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnHelp" alignment="CENTER" layoutX="541.0" layoutY="14.0" mnemonicParsing="false" text="Help">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </Button>
   </children>
</AnchorPane>
