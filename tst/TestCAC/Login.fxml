<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="ancLogin" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TestCAC.UIManager">
   <children>
      <Label fx:id="lblTitle" layoutX="291.0" layoutY="58.0" text="Login">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <VBox fx:id="vbxBox" alignment="CENTER" layoutX="245.0" layoutY="106.0" prefHeight="200.0" prefWidth="150.0">
         <children>
            <TextField fx:id="txtNameL" promptText="Username">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="txtPassL" promptText="Password">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="btnLogin" alignment="CENTER" mnemonicParsing="false" text="Login" onAction="#handleLoginButtonAction">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </Button>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </VBox>
      <Button fx:id="btnHelp" alignment="CENTER" layoutX="541.0" layoutY="14.0" mnemonicParsing="false" text="Help">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </Button>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#handleBackLButtonAction" text="Back" />
   </children>
</AnchorPane>
